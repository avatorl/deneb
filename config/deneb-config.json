{
    "$schema": "../schema/deneb-config.json",
    "features": {
        "developerMode": false,
        "enableExternalUri": false,
        "fetchMoreData": true,
        "selectionContextMenu": true,
        "selectionDataPoint": false,
        "tooltipHandler": true,
        "tooltipResolveNumberFieldFormat": true,
        "unitTestCanary": false
    },
    "fluentUiTheme": {
        "palette": {
            "themePrimary": "#f2c811",
            "themeLighterAlt": "#fefdf5",
            "themeLighter": "#fdf6d7",
            "themeLight": "#fbeeb5",
            "themeTertiary": "#f7de6d",
            "themeSecondary": "#f4cf2c",
            "themeDarkAlt": "#dab50f",
            "themeDark": "#b8990d",
            "themeDarker": "#887109",
            "neutralLighterAlt": "#f3f2f1",
            "neutralLighter": "#eaeaea",
            "neutralLight": "#dadada",
            "neutralQuaternaryAlt": "#dadada",
            "neutralQuaternary": "#d0d0d0",
            "neutralTertiaryAlt": "#c8c8c8",
            "neutralTertiary": "#c2c2c2",
            "neutralSecondary": "#666666",
            "neutralPrimaryAlt": "#4b4b4b",
            "neutralPrimary": "#252423",
            "neutralDark": "#252423",
            "black": "#252423",
            "white": "#ffffff",
            "redDark": "#a4262c"
        }
    },
    "keyBindings": {
        "applyChanges": "ctrl|enter",
        "autoApplyToggle": "ctrl|shift|enter",
        "repairFormatJson": "ctrl|alt|r",
        "newTemplate": "ctrl|alt|e",
        "newSpecification": "ctrl|alt|n",
        "openHelpUrl": "ctrl|alt|h",
        "navigateSpecification": "ctrl|alt|1",
        "navigateConfig": "ctrl|alt|2",
        "navigateSettings": "ctrl|alt|3",
        "toggleEditorPane": "ctrl|alt|space",
        "editorFocusOut": "esc",
        "zoomIn": "ctrl|alt|num_add,ctrl|alt|=",
        "zoomOut": "ctrl|alt|num_subtract,ctrl|alt|-",
        "zoomReset": "ctrl|alt|0,ctrl|alt|num_0",
        "zoomFit": "ctrl|alt|num_multiply"
    },
    "modalDialog": {
        "heightPercent": 0.95,
        "widthPercent": 0.95
    },
    "providerResources": {
        "vega": {
            "documentationUrl": "https://vega.github.io/vega/docs/",
            "schemaUrl": "https://vega.github.io/schema/vega/v5.json",
            "patch": {
                "signals": [
                    {
                        "name": "pbiContainerHeight",
                        "update": "containerSize()[1]"
                    },
                    {
                        "name": "pbiContainerWidth",
                        "update": "containerSize()[0]"
                    }
                ]
            }
        },
        "vegaLite": {
            "documentationUrl": "https://vega.github.io/vega-lite/docs/",
            "schemaUrl": "https://vega.github.io/schema/vega-lite/v5.json"
        }
    },
    "propertyDefaults": {
        "dataLimit": {
            "override": false,
            "showCustomVisualNotes": true
        },
        "developer": { "locale": "en-US" },
        "editor": {
            "debounceInterval": 200,
            "tabSize": 2,
            "position": "left",
            "fontSize": 10,
            "showViewportMarker": true,
            "maxLineLength": 40
        },
        "vega": {
            "jsonSpec": null,
            "jsonConfig": "{}",
            "provider": "vegaLite",
            "renderMode": "svg",
            "enableContextMenu": true,
            "enableTooltips": true,
            "enableSelection": false,
            "isNewDialogOpen": true
        }
    },
    "splitPaneDefaults": {
        "minSize": 350,
        "defaultSizePercent": 0.4,
        "maxSizePercent": 0.6,
        "collapsedSize": 36
    },
    "templates": {
        "metadataVersion": 1
    },
    "visualViewPortAdjust": {
        "top": 5,
        "left": 5
    },
    "zoomLevel": {
        "min": 10,
        "max": 300,
        "step": 10,
        "default": 100
    }
}
