{
    "$schema": "./src/schema/deneb-config.json",
    "features": {
        "developerMode": false,
        "enableExternalUri": false,
        "fetchMoreData": true,
        "selectionContextMenu": false,
        "selectionDataPoint": false,
        "tooltipHandler": true,
        "tooltipResolveNumberFieldFormat": true,
        "unitTestCanary": false
    },
    "keyBindings": {
        "applyChanges": "ctrl+enter",
        "autoApplyToggle": "ctrl+shift+enter",
        "repairFormatJson": "ctrl+alt+r",
        "newTemplate": "ctrl+alt+e",
        "newSpecification": "ctrl+alt+n",
        "openHelpUrl": "ctrl+alt+h",
        "navigateSpecification": "ctrl+alt+1",
        "navigateConfig": "ctrl+alt+2",
        "navigateSettings": "ctrl+alt+3",
        "toggleEditorPane": "ctrl+alt+space",
        "editorFocusOut": "esc"
    },
    "templates": {
        "metadataVersion": 1
    },
    "providerResources": {
        "vega": {
            "documentationUrl": "https://vega.github.io/vega/docs/",
            "schemaUrl": "https://vega.github.io/schema/vega/v5.json"
        },
        "vegaLite": {
            "documentationUrl": "https://vega.github.io/vega-lite/docs/",
            "schemaUrl": "https://vega.github.io/schema/vega-lite/v5.json"
        }
    },
    "propertyDefaults": {
        "dataLimit": {
            "override": false,
            "showCustomVisualNotes": true
        },
        "developer": { "locale": "en-US" },
        "editor": { "debounceInterval": 200, "tabSize": 2, "position": "left" },
        "vega": {
            "jsonSpec": null,
            "jsonConfig": "{}",
            "provider": "vegaLite",
            "renderMode": "svg",
            "enableContextMenu": true,
            "enableTooltips": true,
            "enableSelection": false,
            "isNewDialogOpen": true
        }
    },
    "splitPaneDefaults": {
        "minSize": 350,
        "defaultSizePercent": 0.4,
        "maxSizePercent": 0.6,
        "collapsedSize": 36
    },
    "visualViewPortAdjust": {
        "top": 5,
        "left": 5
    }
}
